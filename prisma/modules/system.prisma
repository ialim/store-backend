// system.prisma

model SystemSetting {
  id          String   @id @default(uuid())
  key         String   @unique
  value       String
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  updatedById String?

  updatedBy   User?    @relation("SystemSettingUpdatedBy", fields: [updatedById], references: [id])
}
